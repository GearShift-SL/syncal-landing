---
import { ANALYTICS } from "astrowind:config";
---

{
  ANALYTICS?.vendors?.googleTagManager?.id ? (
    <noscript>
      <iframe
        src={`https://www.googletagmanager.com/ns.html?id=${ANALYTICS.vendors.googleTagManager.id}`}
        height="0"
        width="0"
        style="display:none;visibility:hidden"
      />
    </noscript>
  ) : null
}
